{
  "name": "home-board",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "bootstrap": "cp node_modules/bootstrap/dist/css/bootstrap.min.css ./dist/assets/css/",
    "bootstrap-material": "cp node_modules/bootstrap-material-design/dist/css/bootstrap-material-design.min.css ./dist/assets/css/",
    "start": "npm run bootstrap && npm run bootstrap-material && webpack-dev-server --progress --colors --hot --config ./webpack.config.js",
